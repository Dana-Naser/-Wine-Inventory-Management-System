<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="GenerateReport" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="32dac894-bcd9-474b-8048-7b6c9989817a">
	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="USA"/>
	<parameter name="FoodItem" class="java.lang.String"/>
	<parameter name="Occasion" class="java.lang.String"/>
	<parameter name="TypeName" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
            TblWines.SerialNumber,
            TblWines.Name AS WineName,
            TblWines.Description,
            TblWines.ProductionYear,
            TblWines.PricePerBottle AS Price,
            TblWines.sweetnessLevel AS Sweetness,
            TblWines.ManufacturerID,
            TblWinesType.SuitableOccasions AS Occasions,
            TblWinesType.Name AS WineType,
            TblFoodPairing.DishName AS FoodPairing
        FROM 
            TblWines
        LEFT JOIN 
            TblWinesType ON TblWines.SerialNumber = TblWinesType.TypeID
        LEFT JOIN 
            TblFoodPairing ON TblWines.SerialNumber = TblFoodPairing.WineID
        WHERE 
            ($P{FoodItem} IS NULL OR TblFoodPairing.DishName LIKE CONCAT('%', $P{FoodItem}, '%'))
            AND ($P{Occasion} IS NULL OR TblWinesType.SuitableOccasions LIKE CONCAT('%', $P{Occasion}, '%'))
            AND ($P{TypeName} IS NULL OR TblWinesType.Name LIKE CONCAT('%', $P{TypeName}, '%'))]]>
	</queryString>
